body {
  background #fff
}
.vue-date-range-wrapper {
  background #d4d7d6
  border 1px solid #f9f9f9
  padding 20px;
  max-width: 400px
  margin 0 auto
  section {
    margin-bottom 20px
  }
  pre {
    margin-top 10px
    font-size 0.8rem
    background #002b36
    color #eecc8b
    overflow-x scroll
  }
  h2 {
    margin-top 10px
    margin-bottom 10px
    text-align center
    font-size 20px
  }
  span.date {
    display block
    width 100%
    margin-bottom 10px
  }
}
#custom-style .ayou-day-cell .selected {
  background-color orange
}
#custom-style .ayou-day-cell .in-range {
  background-color orange
}